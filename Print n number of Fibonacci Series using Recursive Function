#include<stdio.h>  
int fibo(int);  
void main ()  
{  
    int x,f;  
    printf("Enter value of n number?");  
    scanf("%d",&x);  
    f = fibo(x);  
    printf("%d",f);  
}  
int fibo (int x)  
{  
    if (x==0)  
    {  
    return 0;  
    }  
    else if (x == 1)  
    {  
        return 1;   
    }  
    else  
    {  
        return fibo(x-1)+fibo(x-2);  
    }  
}  
